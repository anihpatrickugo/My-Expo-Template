{
  "name": "my-template",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn generate-types && NODE_ENV=development npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "NODE_ENV=development npx expo start --web",
    "start:stage": "NODE_ENV=staging npx expo start",
    "start:production": "npx expo start",
    "android:stage": "NODE_ENV=staging npx expo start --android",
    "ios:stage": "NODE_ENV=staging npx expo start --ios",
    "web:stage": "NODE_ENV=staging npx expo start --web",
    "android:production": "npx expo start --android",
    "ios:production": "npx expo start --ios",
    "web:production": "npx expo start --web",
    "build:android": "npx eas-cli build -e production -p android",
    "build:ios": "npx eas-cli build -e production -p ios",
    "build:all": "npx eas-cli build -e production -p all",
    "build:android-stage": "NODE_ENV=staging npx eas-cli build -e staging -p android",
    "build:ios-stage": "NODE_ENV=staging npx eas-cli build -e staging -p ios",
    "build:all-stage": "NODE_ENV=staging npx eas-cli build -e staging -p all",
    "build:android-dev": "NODE_ENV=development npx eas-cli build -e development -p android",
    "build:ios-dev": "NODE_ENV=development npx eas-cli build -e development -p ios",
    "build:all-dev": "NODE_ENV=development npx eas-cli build -e development -p all",
    "submit:ios": "npx eas-cli submit -p ios --latest",
    "submit:android": "npx eas-cli submit -p android --latest",
    "submit:all": "npx eas-cli submit -p all --latest",
    "submit:ios-stage": "npx eas-cli submit -p ios -e staging --latest",
    "submit:android-stage": "npx eas-cli submit -p android -e staging --latest",
    "submit:all-stage": "npx eas-cli submit -p all -e staging --latest",
    "ota:all": "npx eas-cli update -p all",
    "ota:android": "npx eas-cli update -p android",
    "ota:ios": "npx eas-cli update -p ios",
    "prebuild:all": "npx expo prebuild -p all",
    "prebuild:ios": "npx expo prebuild -p ios",
    "prebuild:android": "npx expo prebuild -p android",
    "devtools": "react-devtools",
    "generate-types": "npx swagger-typescript-api -p https://pickriders-api-dev.onrender.com/docs/api-json -o ./src/services/apiService --debug --responses --axios --route-types --extract-request-params --extract-request-body --extract-response-body --modular --default-response any --patch --unwrap-response-data",
    "lint": "npx tsc && eslint .",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/query-async-storage-persister": "^5.67.2",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-query-persist-client": "^5.67.2",
    "axios": "^1.8.2",
    "expo": "~52.0.37",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.7",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "formik": "^2.4.6",
    "lottie-react-native": "^7.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-full-responsive": "^2.4.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "socket.io-client": "^4.8.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "react-test-renderer": "18.3.1",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "^5.3.3"
  },
  "private": true,
  "packageManager": "yarn@4.6.0"
}
